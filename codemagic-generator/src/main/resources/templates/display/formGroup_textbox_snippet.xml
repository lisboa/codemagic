<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:chzn="urn:import:com.arcbees.chosen.client.gwt">

	<b:FormGroup>
		<b:FormLabel for="${fieldName}" showRequiredIndicator="${required}">${title}</b:FormLabel>
		<b:InlineHelpBlock iconType="EXCLAMATION_TRIANGLE" />
		<b:TextBox ui:field="${componentName}" b:id="${fieldName}" readOnly="${readOnly}" enabled="${enabled}"
			maxLength="${maxLength}" allowBlank="${allowBlank}" tabIndex="${tabIndex}" />
	</b:FormGroup>
</ui:UiBinder>