<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:chzn="urn:import:com.arcbees.chosen.client.gwt">

	<b:Row>
		<b:Column size="MD_12">
			<b:Row marginBottom="20">
				<b:Column size="MD_12" b:id="formContainer">
					<b:Form ui:field="${formName}">
					</b:Form>
				</b:Column>
			</b:Row>
			<b:Row>
				<b:Column size="MD_12">
					<b:ButtonGroup>
						<b:Button text="Apply"  ui:field="btnApply" type="PRIMARY" marginRight="10" tabIndex="${tabIndexApply}" />
						<b:Button text="Save" ui:field="btnSave" type="PRIMARY" marginRight="10" tabIndex="${tabIndexSave}" />
						<b:Button text="Cancel" ui:field="btnCancel" tabIndex="${tabIndexCancel}" />
					</b:ButtonGroup>
				</b:Column>
			</b:Row>
		</b:Column>
	</b:Row>
</ui:UiBinder>