<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:chzn="urn:import:com.arcbees.chosen.client.gwt">

	<b:PanelGroup b:id="accordion">
		<b:Panel>
			<b:PanelHeader>
				<b:Heading size="H4">
					<b:Anchor icon="CHEVRON_DOWN" dataToggle="COLLAPSE"
						dataParent="#accordion" dataTarget="#collapseOne" text="More options"
						iconBordered="true" ui:field="moreOptionsAnchor" tabIndex="${tabIndex}" />
				</b:Heading>
			</b:PanelHeader>
			<b:PanelCollapse b:id="collapseOne" ui:field="collapseOne">
				<b:PanelBody>
					<b:Form ui:field="moreFieldsForm">
						<!-- put formGroup here -->
					</b:Form>
				</b:PanelBody>
			</b:PanelCollapse>
		</b:Panel>
	</b:PanelGroup>
</ui:UiBinder>